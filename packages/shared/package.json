{
    "name": "@glideapps/glide-data-grid-shared",
    "version": "6.0.4-alpha23",
    "description": "Shared utilities for Glide Data Grid across framework targets.",
    "type": "module",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/dts/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/dts/index.d.ts",
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        },
        "./color": {
            "types": "./dist/dts/color/index.d.ts",
            "import": "./dist/esm/color/index.js",
            "require": "./dist/cjs/color/index.js"
        },
        "./geometry": {
            "types": "./dist/dts/geometry/index.d.ts",
            "import": "./dist/esm/geometry/index.js",
            "require": "./dist/cjs/geometry/index.js"
        },
        "./types": {
            "types": "./dist/dts/types.d.ts",
            "import": "./dist/esm/types.js",
            "require": "./dist/cjs/types.js"
        },
        "./grid": {
            "types": "./dist/dts/grid/index.d.ts",
            "import": "./dist/esm/grid/index.js",
            "require": "./dist/cjs/grid/index.js"
        },
        "./animation": {
            "types": "./dist/dts/animation/index.d.ts",
            "import": "./dist/esm/animation/index.js",
            "require": "./dist/cjs/animation/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "./build.sh",
        "lint": "eslint src --ext .ts",
        "test": "vitest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/glideapps/glide-data-grid.git",
        "directory": "packages/shared"
    },
    "author": "Glide",
    "license": "MIT",
    "peerDependencies": {},
    "devDependencies": {}
}
