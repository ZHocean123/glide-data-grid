{
  "name": "@glideapps/glide-data-grid-vue",
  "version": "6.0.4-alpha23",
  "description": "Vue 3 data grid for beautifully displaying and editing large amounts of data with amazing performance.",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "browser": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/dts/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    "./index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "scripts": {
    "build": "./build.sh",
    "lint": "eslint src --ext .ts,.tsx,.vue",
    "test": "vitest",
    "dev": "vite",
    "watch": "chokidar 'src/**/*.{ts,tsx,vue}' -c './build.sh'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glideapps/glide-data-grid.git",
    "directory": "packages/vue-core"
  },
  "keywords": [
    "vue",
    "datagrid",
    "data-grid",
    "editor",
    "vuejs",
    "scrolling",
    "data",
    "table",
    "cell",
    "canvas"
  ],
  "author": "Glide",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glideapps/glide-data-grid/issues"
  },
  "homepage": "https://grid.glideapps.com",
  "dependencies": {
    "@linaria/vue": "^6.3.0",
    "canvas-hypertxt": "^1.0.3",
    "vue": "^3.4.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.19",
    "marked": "^16.0.10",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@glideapps/ts-helper": "6.0.0",
    "@skypack/package-check": "^0.2.2",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.0.0",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.8.3",
    "vite": "^5.0.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.0"
  }
}
